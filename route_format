struct header {
    int center_lat; /* route center latitude   2^31/180 */
    int center_lon; /* route center longtitude 2^31/180 */
    unsigned int npts;	    /* number of points */
    unsigned int nwps;	    /* number of turn descriptions */
} header;
struct coord {
    int x; /* meters from center */
    int y; /* meters from center */
    unsigned int dist; /* meters to end point */
} pts[npts];
struct wp {
    unsigned int idx;        /* index into pts array */
    unsigned int len;        /* strlen(desc); */
    char desc[len]; /* turn information */
} wps[nwps]

